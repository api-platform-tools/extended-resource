{
    "name": "api-platform-tools/extended-resource",
    "description": "API Platform Tools: Extend api-platform/core ApiResource attribute",
    "type": "library",
    "keywords": [
        "api",
        "api-platform",
        "api-platform-tools",
        "api-resource",
        "attribute",
        "php",
        "resource",
        "tools"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simpletoimplement",
            "email": "simpletoimplement@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "api-platform/core": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": ">=10.0"
    },
    "autoload": {
        "psr-4": {
            "ApiPlatformTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiPlatformTools\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-simple": "0.x-dev"
        },
        "thanks": {
            "name": "api-platform-tools/extended-resource",
            "url": "https://github.com/api-platform-tools/extended-resource"
        }
    },
    "funding": [
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/api-platform-tools/extended-resource"
        }
    ]
}
