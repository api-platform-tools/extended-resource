{
    "name": "phpsimple/extended-api-resource",
    "description": "Php Simple Extend api-platform ApiResource attribute",
    "type": "library",
    "keywords": [
        "api",
        "api-platform",
        "api-resource",
        "attribute",
        "php",
        "resource"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simpletoimplement",
            "email": "simpletoimplement@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "api-platform/core": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "spaghetti/php-cs-fixer-config": ">=1.1",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpSimple\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-simple": "1.x-dev"
        },
        "thanks": {
            "name": "phpsimple/extended-api-resource",
            "url": "https://github.com/phpsimple/extended-api-resource"
        }
    },
    "funding": [
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/phpsimple/extended-api-resource"
        }
    ]
}
